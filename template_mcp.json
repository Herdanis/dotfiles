{
  "mcpServers": {
    "jam": {
      "type": "http",
      "url": "https://mcp.jam.dev/mcp"
    },
    "tmux": {
      "command": "npx",
      "args": [
        "-y",
        "tmux-mcp",
        "--shell-type=fish"
      ]
    },
    "browsermcp": {
      "command": "npx",
      "args": [
        "@browsermcp/mcp@latest"
      ]
    },
    "gcp": {
      "command": "sh",
      "args": [
        "-c",
        "npx -y gcp-mcp"
      ]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server"
      ]
    },
    "awslabs.core-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.core-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    }
  }
}
